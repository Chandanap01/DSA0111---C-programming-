#include <iostream>
using namespace std;
int add(int a, int b) {
    return a + b;
}
float add(float a, float b) {
    return a + b;
}
int add(int a, int b, int c) {
    return a + b + c;
}

int main() {
    int x = 10, y = 20, z = 30;
    float p = 5.5, q = 10.5;
    cout << "Addition of two integers: " << add(x, y) << endl;
    cout << "Addition of two floats: " << add(p, q) << endl;
    cout << "Addition of three integers: " << add(x, y, z) << endl;
    return 0;
}
